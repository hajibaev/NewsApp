<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/everythingFragment">

    <fragment
        android:id="@+id/everythingFragment"
        android:name="com.example.ui.all_news.all_everything.EverythingFragment"
        android:label="fragment_everything"
        tools:layout="@layout/fragment_everything" >
        <action
            android:id="@+id/action_everythingFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/topNewsFragment"
        android:name="com.example.ui.all_news.all_top_head_lines.TopNewsFragment"
        android:label="fragment_top_news"
        tools:layout="@layout/fragment_top_news" >
        <action
            android:id="@+id/action_topNewsFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/storageFragment"
        android:name="com.example.ui.all_news_storage.StorageFragment"
        android:label="fragment_storage_frgment"
        tools:layout="@layout/fragment_storage_frgment" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.ui.SettingFragment"
        android:label="SettingFragment" />


</navigation>